{
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "predeploy": "cd example && yarn install && yarn run build",
    "deploy": "gh-pages -b demo -d example/dist"
  },
  "peerDependencies": {
    "react": ">=16.8"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 90,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "react-resizable-element",
  "author": "Muhammad Ali",
  "module": "dist/react-resizable-element.esm.js",
  "size-limit": [
    {
      "path": "dist/react-resizable-element.cjs.production.min.js",
      "limit": "5 KB"
    },
    {
      "path": "dist/react-resizable-element.esm.js",
      "limit": "5 KB"
    }
  ],
  "side-effects": false,
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.10.1",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "gh-pages": "^3.1.0",
    "husky": "^5.2.0",
    "identity-obj-proxy": "^3.0.0",
    "postcss": "^8.2.8",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rollup-plugin-postcss": "^4.0.0",
    "size-limit": "^4.10.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.1.0",
    "typescript": "^4.2.3"
  }
}
